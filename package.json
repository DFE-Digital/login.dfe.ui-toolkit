{
  "name": "dfe-frontend-toolkit",
  "version": "8.0.0",
  "description": "DfE front-end toolkit",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV='development' gulp run-server build-gds",
    "dev:checks": "npm run lint",
    "format": "prettier . --write",
    "build": "gulp build-gds",
    "start": "node server.js",
    "prepare": "husky",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:report": "eslint . -f junit -o eslint-results.xml"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "express": "^4.16.2",
    "govuk_frontend_toolkit": "^9.0.1",
    "govuk_template_jinja": "^0.26.0",
    "govuk-elements-sass": "^3.1.2",
    "govuk-frontend": "^4.8.0",
    "jquery": "^3.6.0",
    "js-cookie": "^3.0.1",
    "nunjucks": "^3.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "browserify": "^17.0.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "eslint": "^9.17.0",
    "eslint-formatter-junit": "^8.40.0",
    "eslint-plugin-jest": "^28.10.0",
    "globals": "^15.14.0",
    "gulp": "^5.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-dart-sass": "^1.0.2",
    "gulp-if": "^3.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-uglify": "^3.0.0",
    "gulp-uglify-es": "^3.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "prettier": "^3.4.2",
    "select2": "^4.0.6-rc.1",
    "through2": "^4.0.2",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  },
  "overrides": {
    "glob-parent": "5.1.2",
    "postcss": "8.4.31",
    "glob-watcher": {
      "chokidar": "3.0.0"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DFE-Digital/frontend-toolkit.git"
  },
  "author": "DfE",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DFE-Digital/frontend-toolkit/issues"
  },
  "homepage": "https://github.com/DFE-Digital/frontend-toolkit#readme"
}
